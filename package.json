  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guiseek/customdoc.git"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "checkcommit": "node ./scripts/commit-lint.js",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prerelease": "nx affected:build --all",
    "release": "nx affected --target release --all",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run checkcommit"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "10.0.10",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "10.0.10",
    "@angular/compiler": "10.0.10",
    "@angular/core": "10.0.10",
    "@angular/forms": "10.0.10",
    "@angular/platform-browser": "10.0.10",
    "@angular/platform-browser-dynamic": "10.0.10",
    "@angular/router": "10.0.10",
    "@nrwl/angular": "10.0.12",
    "marked": "^1.1.1",
    "prismjs": "^1.21.0",
    "rxjs": "6.5.5",
    "tslib": "^2.0.1",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.6",
    "@angular-devkit/build-ng-packagr": "0.1000.6",
    "@angular/cli": "10.0.6",
    "@angular/compiler-cli": "10.0.10",
    "@angular/language-service": "10.0.10",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt": "^9.1.2",
    "@compodoc/compodoc": "^1.1.11",
    "@ngneat/spectator": "^5.13.0",
    "@nrwl/cypress": "10.0.12",
    "@nrwl/jest": "10.0.12",
    "@nrwl/workspace": "10.0.12",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/gitlab": "^6.0.4",
    "@twittwer/compodoc": "^1.3.2",
    "@types/jest": "25.1.4",
    "@types/marked": "^1.1.0",
    "@types/node": "~8.9.4",
    "@types/prismjs": "^1.16.1",
    "codelyzer": "~5.0.1",
    "commitizen": "^4.1.2",
    "cypress": "^4.1.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "ng-packagr": "^10.0.4",
    "prettier": "2.0.4",
    "semantic-release": "^17.1.1",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "3.9.7"
  }
}
